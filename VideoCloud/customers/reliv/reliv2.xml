<!DOCTYPE Runtime SYSTEM "http://admin.brightcove.com/dtds/beml_rt.dtd"><Runtime>  <Theme name="Deluxe" style="Light" bodyFont="Verdana" titleFont="Verdana Bold" linkFont="verdana">    <Style id="default"><![CDATA[        .titleText {        fontSize: 12;        active: #A81E28;        disabled: #A8AAAD;        rollover: #A81E28;      selected: #A81E28;      fontFace: "Verdana";        }        .bodyText {        fontSize: 11;        active: #666666;        disabled: #A8AAAD;        rollover: #666666;        }        .linkText {fontSize: 11;}        ]]></Style><Style id="currentVideoTitle"><![CDATA[      .titleText {      active: #00A4F4;      rollover: #8DC63F;      selected: #00A4F4;      }      .bodyText {      active: #666666;      }      ]]></Style>    <Style class="TileList"><![CDATA[      .titleText {      active: #00A4F4;      disabled: #A1A1A1;      rollover: #8DC63F;      selected: #00B1B0;      }      .bodyText {      active: #666666;      disabled: #A1A1A1;      rollover: #666666;      selected: #666666;      }      .pagingButtons {
  gradientTop: #FFFFFF;
  gradientBottom: #EEEEEE;
  rolloverGradientTop: #F5F5F5;
  rolloverGradientBottom: #DDDDDD;
  selectedGradientTop: #DDDDDD;
  selectedGradientBottom: #F5F5F5;
  disabledGradientTop: #FFFFFF;
  disabledGradientBottom: #FFFFFF;
  borderGradientTop: #00A4E4;
  borderGradientBottom: #00A4E4;
  rolloverBorderGradientTop: #CCCCCC;
  rolloverBorderGradientBottom: #AAAAAA;
  selectedBorderGradientTop: #CCCCCC;
  selectedBorderGradientBottom: #AAAAAA;
  disabledBorderGradientTop: #CCCCCC;
  disabledBorderGradientBottom: #CCCCCC;
  backgroundGradientTop: #E1E1E1;
  backgroundGradientBottom: #D2D2D2;
  backgroundBorderGradientTop: #E1E1E1;
  backgroundBorderGradientBottom: #D2D2D2;
  icons: #00A4E4;
  rolloverIcons: #4B4B4B;
  selectedIcons: #4B4B4B;
  disabledIcons: #888888;
}
.buttons {
  gradientTop: #FDFDFD;
  gradientBottom: #D2D2D2;
  borderGradientTop: #00A4E4;
  borderGradientBottom: #00A4E4;
  rolloverGradientTop: #FDFDFD;
  rolloverGradientBottom: #D2D2D2;
  downStateGradientTop: #FDFDFD;
  downStateGradientBottom: #D2D2D2;
  downStateBorder: #B0C645;
  text: #777777;
  rolloverText: #B0C645;
  disabledText: #AAAAAA;
  icons: #FF0000;
  rolloverIcons: #B0C645;
  disabledIcons: #AAAAAA;
}      ]]></Style>    <Style id="playlistTabs"><![CDATA[    .tabs {      text: #666666;      rolloverText: #8DC63F;      selectedText: #00B1B0;      fontFace: "Verdana";      fontSize: 15;      }      ]]>    </Style>  </Theme>  <Layout id="application" width="700" height="882" backgroundImage="http://files.brightcove.com/reliv-player-background.png" gutter="0" padding="10" boxType="vbox">    <ChromelessVideoPlayer height="388" id="videoPlayer" video="{videoList.selectedItem}"/>
    <Spacer height="8"/>    <HBox>      <VBox width="600" gutter="3">        <TitleLabel id="currentVideoTitle" text="{videoPlayer.video.displayName}" size="24" height="30" truncate="true"/>        <Label text="{videoPlayer.video.shortDescription}" height="54" size="14" multiline="true" truncate="true"/>      </VBox>      <HBox vAlign="top" hAlign="right">        <Label text="Time:" height="14"/>        <Label text="{format(videoPlayer.mediaDuration, SecondsTimecodeFormatter)}" height="14"/>      </HBox>    </HBox>
    <Spacer height="10" />
    <TabBar id="playlistTabs" height="30" labelBuffer="30"/>
        <Canvas id="listBorderBox" padding="10" height="340" backgroundImage="http://files.brightcove.com/reliv-playlist-background.png">        <TileList id="videoList" data="{playlistTabs.selectedItem.videoDTOs}" columnWidth="135" columnGutter="6" rowHeight="152" numRows="2" rowGutter="6" height="320" width="660">      <ListItem boxType="vbox" padding="2" gutter="2">        <ThumbnailButton source="{currentItem.thumbnailURL}" width="130" height="73"/>        <TitleLabel text="{currentItem.displayName}" size="10" height="14" truncate="true"/>        <Label text="{currentItem.shortDescription}" multiline="true" size="10" height="36" truncate="true"/>          <HBox>            <Label text="{format(currentItem.length, MillisecondsTimecodeFormatter)}" size="11" height="12" hAlign="right"/>            <Spacer width="4"/>            </HBox><Spacer height="4"/>      </ListItem>          </TileList>      </Canvas>
  </Layout></Runtime>